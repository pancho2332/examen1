Algoritmo primer1
	Definir Nnegativo,Npositivo,num Como Real;
	Escribir "elege un numro positivo o negativo";
	leer num
	
	si num<0 Entonces
		
		Npositivo=-num
		
		Escribir"a hora es",Npositivo
	FinSi
	
	si num>0 Entonces
		Nnegativo=-num
	FinSi
	Escribir"cambio a", Nnegativo

FinAlgoritmo
